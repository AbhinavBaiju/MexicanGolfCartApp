{
    "name": "mexican-golf-carts-app",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "apps/shopify/*",
        "worker",
        "shared"
    ],
    "scripts": {
        "dev": "concurrently \"npm run dev:shopify\" \"npm run dev:worker\"",
        "dev:shopify": "cd apps/shopify/mexican-golf-cart && npm run dev",
        "dev:worker": "cd worker && npm run dev",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "devDependencies": {
        "concurrently": "^8.0.0"
    },
    "overrides": {
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0"
    }
}