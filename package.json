{
    "name": "mexican-golf-carts-app",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "apps/shopify/*",
        "worker",
        "shared"
    ],
    "scripts": {
        "dev": "concurrently --kill-others-on-fail --names \"worker,shopify,pages\" --prefix-colors \"blue,green,magenta\" \"npm run dev:worker\" \"npm run dev:shopify\" \"npm run dev:pages\"",
        "dev:shopify": "cd apps/shopify/mexican-golf-cart && npm run dev -- --config dev",
        "dev:worker": "node scripts/dev-worker-live.mjs",
        "dev:worker:local": "cd worker && npm run dev",
        "dev:pages": "cd apps/admin && VITE_WORKER_ADMIN_BASE_URL=https://mexican-golf-cart-worker-dev.explaincaption.workers.dev npm run dev -- --host 0.0.0.0 --port 4173 --strictPort",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "devDependencies": {
        "concurrently": "^8.0.0",
        "wrangler": "^4.60.0"
    },
    "overrides": {
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0"
    }
}
